{
    "manifest_version": 3,
    "name": "GitBoard AI",
    "key": "",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "identity"
    ],
    "host_permissions": [
        "https://api.github.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo.png",
            "32": "logo.png",
            "64": "logo.png",
            "128": "logo.png"
        }
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "popup.html",
                "popup.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}